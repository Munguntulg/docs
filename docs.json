            "pages": [
              "tutorials/photoshop-adjustment-layers",
              "tutorials/photoshop-export-file-formats",
              "tutorials/photoshop-keyboard-shortcuts",
              "tutorials/photoshop-layers-masks",
              "tutorials/photoshop-plugins-integrations",
              "tutorials/photoshop-technical-requirements",
              "tutorials/photoshop-troubleshoot-performance",
              "tutorials/photoshop-whats-new",
              "tutorials/photoshop-workspace-overview"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/photoshop-generative-ai",
              "ai-tools/photoshop-plugins-integrations"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
